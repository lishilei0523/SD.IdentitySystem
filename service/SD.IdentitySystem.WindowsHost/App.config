<configuration>
  <configSections>
    <!--EntityFramework节点-->
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
    <!--依赖注入注册配置节点-->
    <section name="injectionRegisterConfiguration" type="SD.IOC.Core.InjectionRegisterConfiguration, SD.IOC.Core" />
    <!--缓存提供者配置节点-->
    <section name="cacheProviderConfiguration" type="SD.CacheManager.CacheProviderConfiguration, SD.CacheManager" />
    <!--日志记录者配置配置节点-->
    <section name="loggerProviderConfiguration" type="SD.AOP.Core.LoggerProviderConfiguration, SD.AOP.Core" />
    <!--Redis服务器配置节点-->
    <section name="redisConfiguration" type="SD.Toolkits.RedisSection, SD.Toolkits.Redis" />
  </configSections>

  <!--缓存提供者配置-->
  <cacheProviderConfiguration type="SD.CacheManager.RedisProvider" assembly="SD.CacheManager.Redis" />

  <!--日志记录者配置配置，默认采用Sql Server记录-->
  <loggerProviderConfiguration type="SD.AOP.Core.Implements.DefaultLogger" assembly="SD.AOP.Core" />

  <!--连接字符串-->
  <connectionStrings>
    <!--默认连接字符串-->
    <add name="DefaultConnection" connectionString="Data Source=.;Initial Catalog=SD.IdentitySystem;User Id=sa;Password=realgoal123!;MultipleActiveResultSets=true;" providerName="System.Data.SqlClient" />
    <!--日志连接字符串-->
    <add name="LogConnection" connectionString="Data Source=.;Initial Catalog=SD.IdentitySystem;User Id=sa;Password=realgoal123!;MultipleActiveResultSets=true;" providerName="System.Data.SqlClient" />
  </connectionStrings>

  <!--应用程序配置-->
  <appSettings>
    <!--服务名称-->
    <add key="ServiceName" value="SD.IdentitySystem.Service" />
    <!--服务显示名称-->
    <add key="ServiceDisplayName" value="SD.IdentitySystem.Service" />
    <!--服务描述-->
    <add key="ServiceDescription" value="身份认证系统服务" />
    <!--编号生成器连接字符串-->
    <!--键：约定；值：connectionString name-->
    <add key="SerialNumberConnection" value="DefaultConnection" />
    <!--实体所在程序集-->
    <add key="EntityAssembly" value="SD.IdentitySystem.Domain" />
    <!--实体配置所在程序集-->
    <add key="EntityConfigAssembly" value="SD.IdentitySystem.Repository" />
    <!--领域事件源所在程序集-->
    <add key="EventSourceAssembly" value="SD.IdentitySystem.Domain" />
    <!--数据表名前缀-->
    <add key="TablePrefix" value="" />
    <!--身份过期时间，单位：分钟-->
    <add key="AuthenticationTimeout" value="20" />
  </appSettings>

  <!--依赖注入注册配置-->
  <injectionRegisterConfiguration>
    <!--As接口程序集列表-->
    <asInterfaceAssemblies>
      <assembly name="SD.IdentitySystem.AppService" />
      <assembly name="SD.IdentitySystem.DomainService" />
      <assembly name="SD.IdentitySystem.DomainEventHandler" />
      <assembly name="SD.IdentitySystem.Repository" />
    </asInterfaceAssemblies>

    <!--As自身程序集列表-->
    <asSelfAssemblies>
      <assembly name="SD.IdentitySystem.AppService" />
    </asSelfAssemblies>

    <!--As接口类型列表-->
    <asInterfaceTypes>
      <type name="SD.Infrastructure.EventStoreProvider.RedisStoreProvider" assembly="SD.Infrastructure.EventStoreProvider.Redis" />
      <type name="SD.Infrastructure.Repository.EntityFramework.DbCleaner" assembly="SD.Infrastructure.Repository.EntityFramework" />
    </asInterfaceTypes>

    <!--As基类类型列表-->
    <asBaseTypes>
      <type name="SD.IdentitySystem.Repository.Base.DbSession" assembly="SD.IdentitySystem.Repository" />
    </asBaseTypes>

    <!--As自身类型列表-->
    <asSelfTypes>
      <type name="SD.IdentitySystem.Domain.Mediators.DomainServiceMediator" assembly="SD.IdentitySystem.Domain" />
      <type name="SD.IdentitySystem.Domain.Mediators.RepositoryMediator" assembly="SD.IdentitySystem.Domain" />
    </asSelfTypes>

    <!--WCF接口列表-->
    <wcfInterfaces>
      <!--<assembly name="" />-->
    </wcfInterfaces>
  </injectionRegisterConfiguration>

  <!--Redis服务器配置-->
  <redisConfiguration password="">
    <!--服务器地址列表-->
    <endpoints>
      <endpoint name="" host="localhost" port="6379" />
    </endpoints>
  </redisConfiguration>

  <!--WCF配置-->
  <system.serviceModel>
    <!--WCF服务配置，手动增加service节点-->
    <services>
      <!--身份认证服务契约配置-->
      <service behaviorConfiguration="DefaultBehavior" name="SD.IdentitySystem.AppService.Implements.AuthenticationContract">
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:4973/Hosts/AuthenticationContract.svc" />
            <add baseAddress="net.tcp://localhost:49730/Hosts/AuthenticationContract.svc" />
            <add baseAddress="net.pipe://localhost/49730/Hosts/AuthenticationContract.svc" />
          </baseAddresses>
        </host>
        <endpoint binding="basicHttpBinding" bindingConfiguration="BasicBinding"
                  contract="SD.IdentitySystem.IAppService.Interfaces.IAuthenticationContract" />
        <endpoint binding="netTcpBinding" bindingConfiguration="TcpBinding"
                  contract="SD.IdentitySystem.IAppService.Interfaces.IAuthenticationContract" />
        <endpoint binding="netNamedPipeBinding" bindingConfiguration="IpcBinding"
                  contract="SD.IdentitySystem.IAppService.Interfaces.IAuthenticationContract" />
      </service>
      <!--用户服务契约配置-->
      <service behaviorConfiguration="DefaultBehavior" name="SD.IdentitySystem.AppService.Implements.UserContract">
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:4973/Hosts/UserContract.svc" />
            <add baseAddress="net.tcp://localhost:49730/Hosts/UserContract.svc" />
            <add baseAddress="net.pipe://localhost/49730/Hosts/UserContract.svc" />
          </baseAddresses>
        </host>
        <endpoint binding="basicHttpBinding" bindingConfiguration="BasicBinding"
                  contract="SD.IdentitySystem.IAppService.Interfaces.IUserContract" />
        <endpoint binding="netTcpBinding" bindingConfiguration="TcpBinding"
                  contract="SD.IdentitySystem.IAppService.Interfaces.IUserContract" />
        <endpoint binding="netNamedPipeBinding" bindingConfiguration="IpcBinding"
                  contract="SD.IdentitySystem.IAppService.Interfaces.IUserContract" />
      </service>
      <!--权限服务契约配置-->
      <service behaviorConfiguration="DefaultBehavior" name="SD.IdentitySystem.AppService.Implements.AuthorizationContract">
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:4973/Hosts/AuthorizationContract.svc" />
            <add baseAddress="net.tcp://localhost:49730/Hosts/AuthorizationContract.svc" />
            <add baseAddress="net.pipe://localhost/49730/Hosts/AuthorizationContract.svc" />
          </baseAddresses>
        </host>
        <endpoint binding="basicHttpBinding" bindingConfiguration="BasicBinding"
                  contract="SD.IdentitySystem.IAppService.Interfaces.IAuthorizationContract" />
        <endpoint binding="netTcpBinding" bindingConfiguration="TcpBinding"
                  contract="SD.IdentitySystem.IAppService.Interfaces.IAuthorizationContract" />
        <endpoint binding="netNamedPipeBinding" bindingConfiguration="IpcBinding"
                  contract="SD.IdentitySystem.IAppService.Interfaces.IAuthorizationContract" />
      </service>
    </services>
    <!--WCF客户端配置，手动增加endpoint节点-->
    <client>
    </client>
    <!--WCF绑定配置，配置好无需修改-->
    <bindings>
      <basicHttpBinding>
        <!--基础绑定配置-->
        <binding name="BasicBinding" maxBufferPoolSize="2147483647" maxBufferSize="2147483647" maxReceivedMessageSize="2147483647" closeTimeout="00:10:00" openTimeout="00:10:00" receiveTimeout="00:10:00" sendTimeout="00:10:00">
          <security mode="None" />
        </binding>
      </basicHttpBinding>
      <wsHttpBinding>
        <!--WS绑定配置-->
        <binding name="WsBinding" maxBufferPoolSize="2147483647" maxReceivedMessageSize="2147483647" closeTimeout="00:10:00" openTimeout="00:10:00" receiveTimeout="00:10:00" sendTimeout="00:10:00">
          <security mode="None">
            <transport clientCredentialType="None" />
            <message clientCredentialType="None" />
          </security>
        </binding>
      </wsHttpBinding>
      <netTcpBinding>
        <!--TCP绑定配置-->
        <binding name="TcpBinding" maxBufferPoolSize="2147483647" maxBufferSize="2147483647" maxReceivedMessageSize="2147483647" closeTimeout="00:10:00" openTimeout="00:10:00" receiveTimeout="00:10:00" sendTimeout="00:10:00">
          <security mode="None">
            <transport clientCredentialType="None" />
            <message clientCredentialType="None" />
          </security>
        </binding>
      </netTcpBinding>
      <netNamedPipeBinding>
        <!--IPC绑定配置-->
        <binding name="IpcBinding" maxBufferPoolSize="2147483647" maxBufferSize="2147483647" maxReceivedMessageSize="2147483647" closeTimeout="00:10:00" openTimeout="00:10:00" receiveTimeout="00:10:00" sendTimeout="00:10:00">
          <security mode="None" />
        </binding>
      </netNamedPipeBinding>
    </bindings>
    <!--WCF行为配置，配置好无需修改-->
    <behaviors>
      <serviceBehaviors>
        <behavior name="DefaultBehavior">
          <!--是否允许get请求访问-->
          <serviceMetadata httpGetEnabled="true" />
          <!--允许从请求消息头中检索元数据地址信息-->
          <useRequestHeadersForMetadataAddress />
          <!--是否显示异常信息-->
          <serviceDebug includeExceptionDetailInFaults="true" />
          <!--最大序列化的对象个数-->
          <dataContractSerializer maxItemsInObjectGraph="2147483647" />
          <!--依赖注入扩展配置-->
          <injection />
          <!--初始化扩展配置-->
          <initialization />
          <!--WCF客户端/服务端身份认证扩展配置-->
          <wcfAuthentication />
        </behavior>
      </serviceBehaviors>
    </behaviors>
    <!--WCF扩展配置，配置好无需修改-->
    <extensions>
      <behaviorExtensions>
        <!--依赖注入扩展配置-->
        <add name="injection" type="SD.IOC.Integration.WCF.IocServiceBehaviorElement, SD.IOC.Integration.WCF" />
        <!--初始化扩展配置-->
        <add name="initialization" type="SD.Infrastructure.WCF.Server.InitializationBehaviorElement, SD.Infrastructure.WCF.Server" />
        <!--WCF客户端/服务端身份认证扩展配置-->
        <add name="wcfAuthentication" type="SD.IdentitySystem.WCFAuthentication.WCF.AuthenticationBehaviorElement, SD.IdentitySystem.WCFAuthentication.WCF" />
      </behaviorExtensions>
    </extensions>
    <!--WCF多宿主绑定配置-->
    <serviceHostingEnvironment multipleSiteBindingsEnabled="true" />
  </system.serviceModel>


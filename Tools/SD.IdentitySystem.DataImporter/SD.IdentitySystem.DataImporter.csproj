<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <TargetFramework>net48</TargetFramework>
    <OutputType>WinExe</OutputType>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>favicon.ico</ApplicationIcon>
    <Authors>Lee</Authors>
    <Company>SD</Company>
    <Description>SD.IdentitySystem 数据导入工具</Description>
    <Copyright>Copyright © SD 2024</Copyright>
    <Version>4.6.2</Version>
    <AssemblyVersion>4.6.2</AssemblyVersion>
    <FileVersion>4.6.2.0</FileVersion>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Platform)'=='AnyCPU'">
    <PlatformTarget>AnyCPU</PlatformTarget>
    <DebugType>full</DebugType>
    <DebugSymbols>true</DebugSymbols>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="favicon.ico" />
    <None Update="Content\Datas\IdentitySystem.xls">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
  </ItemGroup>

  <ItemGroup>
    <Reference Include="System.ServiceModel" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="BusyIndicator" Version="1.9.0">
      <ExcludeAssets>contentFiles</ExcludeAssets>
    </PackageReference>
    <PackageReference Include="FluentWPF" Version="0.10.2" />
    <PackageReference Include="MrAdvice" Version="2.17.0" />
    <PackageReference Include="SD.Common" Version="3.4.0" />
    <PackageReference Include="SD.Toolkits.Json" Version="2.1.0" />
    <PackageReference Include="SD.Toolkits.Excel" Version="2.1.0" />
    <PackageReference Include="SD.IOC.Core" Version="1.3.2" />
    <PackageReference Include="SD.Infrastructure" Version="3.8.1" />
    <PackageReference Include="SD.Infrastructure.WPF" Version="1.7.13" />
    <PackageReference Include="SD.Infrastructure.WPF.Caliburn" Version="1.3.0" />
    <PackageReference Include="SD.IdentitySystem.IAppService" Version="4.6.2" />
    <PackageReference Include="SD.IdentitySystem.WCF.Authentication.Common" Version="2.2.0" />
  </ItemGroup>
</Project>

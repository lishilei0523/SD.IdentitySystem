<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sd="https://github.com/lishilei0523/SD.Framework">

  <!--枚举成员转换器-->
  <sd:EnumMemberConverter x:Key="Converters.EnumMemberConverter" />
  <!--停/启用描述转换器-->
  <sd:BooleanDescriptionConverter x:Key="Converters.EnabledDescriptionConverter">
    <x:String>已停用</x:String>
    <x:String>已启用</x:String>
  </sd:BooleanDescriptionConverter>
  <!--是/否描述转换器-->
  <sd:BooleanDescriptionConverter x:Key="Converters.YesNoDescriptionConverter">
    <x:String>否</x:String>
    <x:String>是</x:String>
  </sd:BooleanDescriptionConverter>
</ResourceDictionary>
